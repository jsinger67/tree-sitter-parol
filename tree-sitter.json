{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tree_sitter_parol",
      "camelcase": "TreeSitterParol",
      "title": "Parol",
      "scope": "source.tree_sitter_parol",
      "file-types": [
        "par"
      ],
      "injection-regex": "^tree_sitter_parol$",
      "class-name": "TreeSitterTreeSitterParol"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter parser for parol language",
    "authors": [
      {
        "name": "Joerg Singer",
        "email": "singer.joerg@gmx.de"
      }
    ],
    "links": {
      "repository": "https://github.com/jsinger67/parol"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}