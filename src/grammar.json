{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "parol",
  "rules": {
    "parol": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "prolog"
        },
        {
          "type": "SYMBOL",
          "name": "grammar_definition"
        }
      ]
    },
    "prolog": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "start_declaration"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "declaration"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "scanner_state"
          }
        }
      ]
    },
    "start_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%start"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%title"
            },
            {
              "type": "SYMBOL",
              "name": "string"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%comment"
            },
            {
              "type": "SYMBOL",
              "name": "string"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%user_type"
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "SYMBOL",
              "name": "user_type_name"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%nt_type"
            },
            {
              "type": "FIELD",
              "name": "nt_name",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              }
            },
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "FIELD",
              "name": "nt_type",
              "content": {
                "type": "SYMBOL",
                "name": "user_type_name"
              }
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%t_type"
            },
            {
              "type": "FIELD",
              "name": "t_type",
              "content": {
                "type": "SYMBOL",
                "name": "user_type_name"
              }
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%grammar_type"
            },
            {
              "type": "SYMBOL",
              "name": "raw_string"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "scanner_directives"
        }
      ]
    },
    "scanner_directives": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%line_comment"
            },
            {
              "type": "SYMBOL",
              "name": "token_literal"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%block_comment"
            },
            {
              "type": "SYMBOL",
              "name": "token_literal"
            },
            {
              "type": "SYMBOL",
              "name": "token_literal"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "%auto_newline_off"
        },
        {
          "type": "STRING",
          "value": "%auto_ws_off"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%on"
            },
            {
              "type": "SYMBOL",
              "name": "identifier_list"
            },
            {
              "type": "SYMBOL",
              "name": "scanner_state_directives"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "%allow_unmatched"
        }
      ]
    },
    "scanner_state_directives": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%enter"
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%push"
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "%pop"
        }
      ]
    },
    "grammar_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%%"
        },
        {
          "type": "SYMBOL",
          "name": "production"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "production"
          }
        }
      ]
    },
    "double_colon": {
      "type": "STRING",
      "value": "::"
    },
    "production": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "alternations"
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "|"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "alternations"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "alternations": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "alternation"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "alternation"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "|"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "alternation": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "factor"
      }
    },
    "factor": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "group"
        },
        {
          "type": "SYMBOL",
          "name": "repeat"
        },
        {
          "type": "SYMBOL",
          "name": "optional"
        },
        {
          "type": "SYMBOL",
          "name": "symbol"
        }
      ]
    },
    "symbol": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "non_terminal"
        },
        {
          "type": "SYMBOL",
          "name": "simple_token"
        },
        {
          "type": "SYMBOL",
          "name": "token_with_states"
        }
      ]
    },
    "token_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "raw_string"
        },
        {
          "type": "SYMBOL",
          "name": "regex"
        }
      ]
    },
    "token_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "token_literal"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "look_ahead"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "simple_token": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "token_expression"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "ast_control"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "token_with_states": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "SYMBOL",
          "name": "identifier_list"
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "SYMBOL",
          "name": "token_expression"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "ast_control"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "\"(\\\\.|[^\"])*\""
    },
    "raw_string": {
      "type": "PATTERN",
      "value": "'(\\\\.|[^'])*'"
    },
    "regex": {
      "type": "PATTERN",
      "value": "\\/(\\\\.|[^\\/])*\\/"
    },
    "group": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "alternations"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "optional": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "alternations"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "repeat": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "alternations"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "non_terminal": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "ast_control"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "scanner_state": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%scanner"
        },
        {
          "type": "FIELD",
          "name": "state_name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "scanner_directives"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "identifier_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        }
      ]
    },
    "ast_control": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "cut_operator"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "member_name"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "user_type_declaration"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "user_type_declaration"
        }
      ]
    },
    "member_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "cut_operator": {
      "type": "STRING",
      "value": "^"
    },
    "user_type_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "user_type_name"
        }
      ]
    },
    "user_type_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "double_colon"
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        }
      ]
    },
    "look_ahead": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "positive_lookahead"
            },
            {
              "type": "SYMBOL",
              "name": "negative_lookahead"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "token_literal"
        }
      ]
    },
    "positive_lookahead": {
      "type": "STRING",
      "value": "?="
    },
    "negative_lookahead": {
      "type": "STRING",
      "value": "?!"
    },
    "line_comment": {
      "type": "PATTERN",
      "value": "\\/\\/[^\\r\\n]*"
    },
    "block_comment": {
      "type": "PATTERN",
      "value": "\\/\\*[^*]*\\*+([^/*][^*]*\\*+)*\\/"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    },
    {
      "type": "SYMBOL",
      "name": "block_comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}